diff --git a/lang/swift/Makefile b/lang/swift/Makefile
new file mode 100644
index 00000000000..46e2bd111a7
--- /dev/null
+++ b/lang/swift/Makefile
@@ -0,0 +1,321 @@
+COMMENT-main =		swift programming language
+COMMENT-stdlib =	swift standard libraries
+COMMENT-libdispatch =	swift dispatch library
+COMMENT-foundation =	swift foundation library
+COMMENT-swiftpm =	swift package manager
+COMMENT-xctest =	swift xctest library
+COMMENT-testing =	swift testing library
+V =			6.2.0
+PKGNAME-main = 		swift-${V}
+PKGNAME-stdlib = 	swift-stdlib-${V}
+PKGNAME-libdispatch = 	swift-libdispatch-${V}
+PKGNAME-foundation = 	swift-foundation-${V}
+PKGNAME-swiftpm = 	swift-swiftpm-${V}
+PKGNAME-xctest = 	swift-xctest-${V}
+PKGNAME-testing = 	swift-testing-${V}
+CATEGORIES =		lang
+HOMEPAGE =		https://swift.org
+
+PERMIT_PACKAGE =	Yes
+
+MULTI_PACKAGES =	-main -stdlib -libdispatch -foundation -swiftpm \
+			-xctest -testing
+
+DIST_SUBDIR =		swift
+
+SEPARATE_BUILD =	Yes
+ONLY_FOR_ARCHS = 	amd64 arm64
+
+FLAVOR ?=
+PSEUDO_FLAVORS =	native_bootstrap
+
+.if ${FLAVOR} == native_bootstrap
+BUILD_DEPENDS +=	lang/swift
+.endif
+
+BUILD_DEPENDS +=	shells/bash
+#BUILD_DEPENDS +=	devel/cmake
+BUILD_DEPENDS +=	devel/ninja
+#BUILD_DEPENDS +=	lang/python/3
+BUILD_DEPENDS +=	sysutils/e2fsprogs
+BUILD_DEPENDS +=	textproc/libxml
+BUILD_DEPENDS +=	net/curl
+
+RUN_DEPENDS-main +=		lang/swift,-stdlib
+RUN_DEPENDS-main +=		lang/swift,-libdispatch
+RUN_DEPENDS-main +=		lang/swift,-foundation
+RUN_DEPENDS-main += 		archivers/zstd
+RUN_DEPENDS-main +=		databases/sqlite3
+RUN_DEPENDS-main +=		sysutils/e2fsprogs
+
+LIB_DEPENDS-stdlib +=		archivers/zstd
+LIB_DEPENDS-stdlib +=		sysutils/e2fsprogs
+
+LIB_DEPENDS-dispatch +=		lang/swift,-stdlib
+
+LIB_DEPENDS-foundation +=	lang/swift,-stdlib
+LIB_DEPENDS-foundation +=	lang/swift,-libdispatch
+LIB_DEPENDS-foundation +=	net/curl
+LIB_DEPENDS-foundation += 	textproc/libxml
+
+LIB_DEPENDS-xctest +=		lang/swift,-stdlib
+LIB_DEPENDS-xctest +=		lang/swift,-libdispatch
+LIB_DEPENDS-xctest +=		lang/swift,-foundation
+LIB_DEPENDS-xctest += 		archivers/zstd
+
+LIB_DEPENDS-testing +=		lang/swift,-libdispatch
+LIB_DEPENDS-testing +=		lang/swift,-foundation
+LIB_DEPENDS-testing +=		lang/swift,-stdlib
+
+RUN_DEPENDS-swiftpm +=		lang/swift,-main
+RUN_DEPENDS-swiftpm +=		lang/swift,-stdlib
+RUN_DEPENDS-swiftpm +=		lang/swift,-libdispatch
+RUN_DEPENDS-swiftpm +=		lang/swift,-foundation
+RUN_DEPENDS-swiftpm +=		lang/swift,-xctest
+RUN_DEPENDS-swiftpm +=		databases/sqlite3
+RUN_DEPENDS-swiftpm += 		textproc/libxml
+
+WRKDIST = 		${WRKDIR}/dist
+PORTHOME = 		${WRKDIR}
+DPB_PROPERTIES =        parallel
+
+LLVM_SPELLING_ARCH =	${MACHINE_ARCH}
+.if "${MACHINE_ARCH}" == "amd64"
+LLVM_SPELLING_ARCH =	x86_64
+.endif
+SWIFT_TRIPLE =		${LLVM_SPELLING_ARCH}-unknown-openbsd${OSREV}
+SUBST_VARS +=		LLVM_SPELLING_ARCH SWIFT_TRIPLE
+
+DISTNAME =		swift-${V}
+TAGNAME = 		release/${V}
+LLVM_TAGNAME =		stable/20240723
+DISPATCH_TAGNAME = 	swift-DEVELOPMENT-SNAPSHOT-2025-10-02-a
+ARGPARSER_TAGNAME = 	1.4.0
+COLLECTIONS_TAGNAME = 	1.3.0
+CRYPTO_TAGNAME = 	3.15.1
+CERTIFICATES_TAGNAME = 	1.10.0
+ASN1_TAGNAME = 		1.3.2
+SYSTEM_TAGNAME = 	1.5.0
+SQLITE_TAGNAME = 	1.0.1
+CMAKE_TAGNAME = 	3.30.2
+SITES =			https://github.com/
+DISTFILES = 		swiftlang/swift/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/llvm-project/archive/refs/heads/swift/${TAGNAME}.tar.gz \
+			swiftlang/swift-cmark/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/swift-llbuild/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/swift-build/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/swift-driver/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/swift-tools-support-core/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/swift-package-manager/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/swift-syntax/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/swift-corelibs-xctest/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/swift-corelibs-foundation/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/swift-corelibs-libdispatch/archive/refs/tags/${DISPATCH_TAGNAME}.tar.gz \
+			swiftlang/swift-foundation-icu/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/swift-foundation/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/swift-experimental-string-processing/archive/refs/heads/${TAGNAME}.tar.gz \
+			swiftlang/swift-toolchain-sqlite/archive/refs/tags/${SQLITE_TAGNAME}.tar.gz \
+			swiftlang/swift-testing/archive/refs/heads/${TAGNAME}.tar.gz \
+			apple/swift-argument-parser/archive/refs/tags/${ARGPARSER_TAGNAME}.tar.gz \
+			apple/swift-collections/archive/refs/tags/${COLLECTIONS_TAGNAME}.tar.gz \
+			apple/swift-crypto/archive/refs/tags/${CRYPTO_TAGNAME}.tar.gz \
+			apple/swift-certificates/archive/refs/tags/${CERTIFICATES_TAGNAME}.tar.gz \
+			apple/swift-asn1/archive/refs/tags/${ASN1_TAGNAME}.tar.gz \
+			apple/swift-system/archive/refs/tags/${SYSTEM_TAGNAME}.tar.gz
+
+BOOTSTRAP_AMD64_SITE =	https://ghcr.io/v2/3405691582/swift-pkg-amd64/blobs/
+BOOTSTRAP_AMD64_HASH = 	sha256:d289ba1226d2018ede4dcf4a10f4c32e03a99d7d10439e691621e6debc33076f
+BOOTSTRAP_ARM64_SITE =	https://ghcr.io/v2/3405691582/swift-pkg-arm64/blobs/
+BOOTSTRAP_ARM64_HASH = 	sha256:b927efd6bd8b1f0e0a455d60a72689a5dbfa93d365fc9e5848e0a6a647675bf0
+
+CMAKE_BIN = 		${WRKDIR}/bin/cmake
+NINJA_BIN = 		${WRKDIR}/bin/ninja
+BOOTSTRAP_DIR =		${WRKBUILD}/install2/${TRUEPREFIX}
+
+post-extract:
+	ln -fs `which cmake` ${CMAKE_BIN}
+	ln -fs `which ninja` ${NINJA_BIN}
+	mkdir -p ${WRKDIST}
+	mv -f ${WRKDIR}/llvm-project-swift-${TAGNAME:S/\//-/} \
+	    ${WRKSRC}/llvm-project
+	mv -f ${WRKDIR}/swift-build-${TAGNAME:S/\//-/} ${WRKSRC}/swift-build
+	mv -f ${WRKDIR}/swift-cmark-${TAGNAME:S/\//-/} ${WRKSRC}/cmark
+	mv -f ${WRKDIR}/swift-corelibs-foundation-${TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-corelibs-foundation
+	mv -f ${WRKDIR}/swift-corelibs-libdispatch-${DISPATCH_TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-corelibs-libdispatch
+	mv -f ${WRKDIR}/swift-corelibs-xctest-${TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-corelibs-xctest
+	mv -f ${WRKDIR}/swift-driver-${TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-driver
+	mv -f \
+	    ${WRKDIR}/swift-experimental-string-processing-${TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-experimental-string-processing
+	mv -f ${WRKDIR}/swift-foundation-icu-${TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-foundation-icu
+	mv -f ${WRKDIR}/swift-foundation-${TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-foundation
+	mv -f ${WRKDIR}/swift-llbuild-${TAGNAME:S/\//-/} ${WRKSRC}/llbuild
+	mv -f ${WRKDIR}/swift-syntax-${TAGNAME:S/\//-/} ${WRKSRC}/swift-syntax
+	mv -f ${WRKDIR}/swift-tools-support-core-${TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-tools-support-core
+	mv -f ${WRKDIR}/swift-testing-${TAGNAME:S/\//-/} ${WRKSRC}/swift-testing
+	mv -f ${WRKDIR}/swift-argument-parser-${ARGPARSER_TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-argument-parser
+	mv -f ${WRKDIR}/swift-asn1-${ASN1_TAGNAME:S/\//-/} ${WRKSRC}/swift-asn1
+	mv -f ${WRKDIR}/swift-certificates-${CERTIFICATES_TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-certificates
+	mv -f ${WRKDIR}/swift-collections-${COLLECTIONS_TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-collections
+	mv -f ${WRKDIR}/swift-crypto-${CRYPTO_TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-crypto
+	mv -f ${WRKDIR}/swift-system-${SYSTEM_TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-system
+	mv -f ${WRKDIR}/swift-toolchain-sqlite-${SQLITE_TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swift-toolchain-sqlite
+	mv -f ${WRKDIR}/swift-package-manager-${TAGNAME:S/\//-/} \
+	    ${WRKSRC}/swiftpm
+	mv -f ${WRKDIR}/swift-${TAGNAME:S/\//-/} ${WRKSRC}/swift
+
+pre-build:
+.if ${FLAVOR} != native_bootstrap
+	if [ ! -e ${BOOTSTRAP_DIR} ]; then \
+	  env SWIFT_BUILD_ROOT=${WRKBUILD} \
+	    ${WRKSRC}/swift/utils/build-script \
+	      -j${MAKE_JOBS} \
+	      --preset=bootstrap_stage0_openbsd \
+	      build_subdir=stage0 install_destdir=${WRKBUILD}/install0; \
+	  mkdir -p ${WRKBUILD}/stage1 && \
+	    [ ! -e ${WRKBUILD}/stage1/llvm-openbsd-${LLVM_SPELLING_ARCH} ] && \
+	    ln -s ${WRKBUILD}/stage0/llvm-openbsd-${LLVM_SPELLING_ARCH}/ \
+		${WRKBUILD}/stage1/llvm-openbsd-${LLVM_SPELLING_ARCH} || true; \
+	  env SWIFT_BUILD_ROOT=${WRKBUILD} \
+	    PATH=${WRKBUILD}/install0/${TRUEPREFIX}/bin/:${PATH} \
+	    ${WRKSRC}/swift/utils/build-script \
+	      -j${MAKE_JOBS} \
+	      --preset=bootstrap_stage1_openbsd \
+	      build_subdir=stage1 install_destdir=${WRKBUILD}/install1; \
+	  mkdir -p ${WRKBUILD}/stage2 && \
+	    [ ! -e ${WRKBUILD}/stage2/llvm-openbsd-${LLVM_SPELLING_ARCH} ] && \
+	    ln -s ${WRKBUILD}/stage0/llvm-openbsd-${LLVM_SPELLING_ARCH}/ \
+		${WRKBUILD}/stage2/llvm-openbsd-${LLVM_SPELLING_ARCH} || true; \
+	  env SWIFT_BUILD_ROOT=${WRKBUILD} \
+	    PATH=${WRKBUILD}/install1/${TRUEPREFIX}/bin/:${PATH} \
+	    ${WRKSRC}/swift/utils/build-script \
+	      -j${MAKE_JOBS} \
+	      --preset=bootstrap_stage2_openbsd \
+	      build_subdir=stage2 install_destdir=${WRKBUILD}/install2; \
+	fi
+.endif
+
+do-build:
+	${SETENV} ${MAKE_ENV} \
+	    PATH=${BOOTSTRAP_DIR}/bin:${PORTPATH} \
+	    SWIFT_BUILD_ROOT=${WRKBUILD} \
+	    ${WRKSRC}/swift/utils/build-script \
+	      -j${MAKE_JOBS} \
+	      --native-clang-tools-path=${WRKDIR}/bin \
+	      --build-subdir=toolchain \
+	      --extra-cmake-options=-DBRIDGING_MODE=PURE \
+	      --install-prefix=${TRUEPREFIX} \
+	      --llvm-install-components=IndexStore \
+	      --llvm-targets-to-build=host \
+	      --release -A \
+	      --install-swift \
+	      --install-libdispatch \
+	      --foundation --install-foundation \
+	      --xctest --install-xctest \
+	      --swift-testing --install-swift-testing \
+	      --swift-testing-macros --install-swift-testing-macros \
+	      --llbuild \
+	      --swiftpm --install-swiftpm \
+	      --swift-driver --install-swift-driver
+
+do-install:
+	${SETENV} DESTDIR=${DESTDIR} ${CMAKE_BIN} --build \
+	    ${WRKBUILD}/toolchain/swift-openbsd-${LLVM_SPELLING_ARCH} -- \
+	    install-swift-components
+	${SETENV} DESTDIR=${DESTDIR} ${CMAKE_BIN} --build \
+	    ${WRKBUILD}/toolchain/libdispatch-openbsd-${LLVM_SPELLING_ARCH} -- \
+	    install
+	${SETENV} DESTDIR=${DESTDIR} ${CMAKE_BIN} --build \
+	    ${WRKBUILD}/toolchain/libdispatch_static-openbsd-${LLVM_SPELLING_ARCH} -- install
+	${SETENV} DESTDIR=${DESTDIR} ${CMAKE_BIN} --build \
+	    ${WRKBUILD}/toolchain/foundation_macros-openbsd-${LLVM_SPELLING_ARCH} \
+	    -- install
+	${SETENV} DESTDIR=${DESTDIR} ${CMAKE_BIN} --build \
+	    ${WRKBUILD}/toolchain/foundation-openbsd-${LLVM_SPELLING_ARCH} -- \
+	    install
+	${SETENV} DESTDIR=${DESTDIR} ${CMAKE_BIN} --build \
+	    ${WRKBUILD}/toolchain/foundation_static-openbsd-${LLVM_SPELLING_ARCH} \
+	    -- install
+	cp ${WRKBUILD}/toolchain/llvm-openbsd-${LLVM_SPELLING_ARCH}/lib/libIndexStore.so \
+	    ${DESTDIR}/${TRUEPREFIX}/lib
+	${SETENV} DESTDIR=${DESTDIR} ${CMAKE_BIN} --build \
+	    ${WRKBUILD}/toolchain/xctest-openbsd-${LLVM_SPELLING_ARCH} -- install
+	${SETENV} DESTDIR=${DESTDIR} ${CMAKE_BIN} --build \
+	    ${WRKBUILD}/toolchain/llbuild-openbsd-${LLVM_SPELLING_ARCH} -- \
+	    install-swift-build-tool
+	${SETENV} DESTDIR=${DESTDIR}/${TRUEPREFIX} ${CMAKE_BIN} --build \
+	    ${WRKBUILD}/toolchain/swifttestingmacros-openbsd-${LLVM_SPELLING_ARCH} -- install
+	${SETENV} DESTDIR=${DESTDIR}/${TRUEPREFIX} ${CMAKE_BIN} --build \
+	    ${WRKBUILD}/toolchain/swifttesting-openbsd-${LLVM_SPELLING_ARCH} -- \
+	    install
+	${WRKDIST}/swiftpm/Utilities/bootstrap install \
+	    --install-only --release \
+	    --swiftc-path ${DESTDIR}/${TRUEPREFIX}/bin/swiftc \
+	    --clang-path ${WRKDIR}/bin/clang \
+	    --cmake-path ${CMAKE_BIN} \
+	    --ninja-path ${NINJA_BIN} \
+	    --build-dir \
+		${WRKBUILD}/toolchain/swiftpm-openbsd-${LLVM_SPELLING_ARCH} \
+	    --llbuild-build-dir \
+		${WRKBUILD}/toolchain/llbuild-openbsd-${LLVM_SPELLING_ARCH} \
+	    --dispatch-build-dir \
+		${WRKBUILD}/toolchain/libdispatch-openbsd-${LLVM_SPELLING_ARCH} \
+	    --prefix ${DESTDIR}/${TRUEPREFIX} \
+	    --verbose
+	${WRKSRC}/swift-driver/Utilities/build-script-helper.py install \
+	    --package-path ${WRKSRC}/swift-driver \
+	    --build-path ${WRKBUILD}/toolchain/swiftdriver-openbsd-${LLVM_SPELLING_ARCH} \
+	    --configuration release \
+	    --toolchain ${DESTDIR}/${TRUEPREFIX} \
+	    --ninja-bin ${NINJA_BIN} \
+	    --cmake-bin ${CMAKE_BIN} \
+	    --dispatch-build-dir \
+		${WRKBUILD}/toolchain/libdispatch-openbsd-${LLVM_SPELLING_ARCH}
+
+# This target downloads a prebuilt Swift toolchain for bootstrapping,
+# when a Swift toolchain is not already available.
+#
+# Since this downloads a container layer, passing an Authorization header
+# is required, and therefore cannot be part of DISTFILES since ftp(1)
+# doesn't know how to supply user-specified headers.
+.PHONY: bootstrap
+bootstrap: extract
+	ln -fs `which curl` ${WRKDIR}/bin/curl
+.if ${MACHINE_ARCH} == "amd64"
+	${WRKDIR}/bin/curl -H "Authorization: Bearer QQ==" -L -C - -o \
+	    ${WRKDIST}/bootstrap-${MACHINE_ARCH}.tar.gz \
+	    ${BOOTSTRAP_AMD64_SITE}/${BOOTSTRAP_AMD64_HASH}
+.else
+	${WRKDIR}/bin/curl -H "Authorization: Bearer QQ==" -L -C - -o \
+	    ${WRKDIST}/bootstrap-${MACHINE_ARCH}.tar.gz \
+	    ${BOOTSTRAP_ARM64_SITE}/${BOOTSTRAP_ARM64_HASH}
+.endif
+	tar xzf ${WRKDIST}/bootstrap-${MACHINE_ARCH}.tar.gz -C ${WRKDIR}
+	mkdir -p ${BOOTSTRAP_DIR}
+	tar xzf \
+	    ${WRKDIR}/${TRUEPREFIX}/share/swift/swift-6.2.0.tgz \
+	    -C ${BOOTSTRAP_DIR}
+	tar xzf \
+	    ${WRKDIR}/${TRUEPREFIX}/share/swift/swift-foundation-6.2.0.tgz \
+	    -C ${BOOTSTRAP_DIR}
+	tar xzf \
+	    ${WRKDIR}/${TRUEPREFIX}/share/swift/swift-libdispatch-6.2.0.tgz \
+	    -C ${BOOTSTRAP_DIR}
+	tar xzf \
+	    ${WRKDIR}/${TRUEPREFIX}/share/swift/swift-stdlib-6.2.0.tgz \
+	    -C ${BOOTSTRAP_DIR}
+
+.include <bsd.port.mk>
